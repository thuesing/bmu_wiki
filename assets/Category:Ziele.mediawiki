<PageSchema>
  <semanticforms_Form name="Ziel">
    <CreateTitle>Neues Ziel</CreateTitle>
    <EditTitle>Ziel editieren</EditTitle>
  </semanticforms_Form>
  <Template name="Ziel" format="standard">
    <Field name="Hat Thema">
      <Label>Hat Thema</Label>
      <semanticmediawiki_Property name="">
        <Type>Page</Type>
      </semanticmediawiki_Property>
      <semanticforms_FormInput>
        <InputType>dropdown</InputType>
        <Parameter name="values from category">Themen</Parameter>
        <Parameter name="size">50</Parameter>
        <Parameter name="mandatory"/>
      </semanticforms_FormInput>
      <semanticdrilldown_Filter/>
    </Field>
    <Field name="Hat Massnahme" list="list">
      <Label>Massnahmen</Label>
      <semanticmediawiki_Property name="">
        <Type>Page</Type>
      </semanticmediawiki_Property>
      <semanticforms_FormInput>
        <InputType>two listboxes</InputType>
        <Parameter name="values from category">Massnahmen</Parameter>
      </semanticforms_FormInput>
    </Field>
    <Field name="Hat Problem" list="list">
      <Label>Probleme</Label>
      <semanticmediawiki_Property name="">
        <Type>Page</Type>
      </semanticmediawiki_Property>
      <semanticforms_FormInput>
        <InputType>two listboxes</InputType>
        <Parameter name="values from category">Probleme</Parameter>
      </semanticforms_FormInput>
    </Field>
    <Field name="Hat Sektor" list="list">
      <Label>Sektoren</Label>
      <semanticmediawiki_Property name="">
        <Type>Page</Type>
      </semanticmediawiki_Property>
      <semanticforms_FormInput>
        <InputType>two listboxes</InputType>
        <Parameter name="values from category">Sektoren</Parameter>
      </semanticforms_FormInput>
    </Field>
  </Template>
</PageSchema>