<PageSchema>
  <semanticforms_Form name="Massnahme">
    <CreateTitle>Neue Massnahme</CreateTitle>
    <EditTitle>Massnahme editieren</EditTitle>
  </semanticforms_Form>
  <Template name="Massnahme" format="standard">
    <Field name="Hat Thema">
      <Label>Hat Thema</Label>
      <semanticmediawiki_Property name="">
        <Type>Page</Type>
      </semanticmediawiki_Property>
      <semanticforms_FormInput>
        <InputType>dropdown</InputType>
        <Parameter name="values from category">Themen</Parameter>
        <Parameter name="size">50</Parameter>
        <Parameter name="mandatory"/>
      </semanticforms_FormInput>
      <semanticdrilldown_Filter/>
    </Field>
    <Field name="Hat Verhalten" list="list">
      <Label>Verhalten</Label>
      <semanticmediawiki_Property name="">
        <Type>Page</Type>
      </semanticmediawiki_Property>
      <semanticforms_FormInput>
        <InputType>two listboxes</InputType>
        <Parameter name="values from category">Verhalten</Parameter>
      </semanticforms_FormInput>
    </Field>
    <Field name="Hat Ziel" list="list">
      <Label>Ziele</Label>
      <semanticmediawiki_Property name="">
        <Type>Page</Type>
      </semanticmediawiki_Property>
      <semanticforms_FormInput>
        <InputType>two listboxes</InputType>
        <Parameter name="values from category">Ziele</Parameter>
      </semanticforms_FormInput>
    </Field>
    <Field name="Hat Sektor" list="list">
      <Label>Sektoren</Label>
      <semanticmediawiki_Property name="">
        <Type>Page</Type>
      </semanticmediawiki_Property>
      <semanticforms_FormInput>
        <InputType>two listboxes</InputType>
        <Parameter name="values from category">Sektoren</Parameter>
      </semanticforms_FormInput>
    </Field>
  </Template>
</PageSchema>