<PageSchema>
  <semanticforms_Form name="Problem">
    <CreateTitle>Neues Problem</CreateTitle>
    <EditTitle>Problem editieren</EditTitle>
  </semanticforms_Form>
  <Template name="Problem" format="standard">
    <Field name="Thema">
      <Label>gehört zu Thema</Label>
      <semanticmediawiki_Property name="">
        <Type>Page</Type>
      </semanticmediawiki_Property>
      <semanticforms_FormInput>
        <InputType>dropdown</InputType>
        <Parameter name="values from category">Themen</Parameter>
      </semanticforms_FormInput>
    </Field>
    <Field name="Ursache" list="list">
      <Label>verstärkt durch Ursachen</Label>
      <semanticmediawiki_Property name="">
        <Type>Page</Type>
      </semanticmediawiki_Property>
      <semanticforms_FormInput>
        <InputType>two listboxes</InputType>
        <Parameter name="values from category">Ursachen</Parameter>
      </semanticforms_FormInput>
    </Field>
    <Field name="Ziel" list="list">
      <Label>adressiert durch Ziele</Label>
      <semanticmediawiki_Property name="">
        <Type>Page</Type>
      </semanticmediawiki_Property>
      <semanticforms_FormInput>
        <InputType>two listboxes</InputType>
        <Parameter name="values from category">Ziele</Parameter>
      </semanticforms_FormInput>
    </Field>
  </Template>
</PageSchema>

